<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.bo.ewm.entity.Dictionaries" table="e_dictionaries" schema="ewm">
        <id name="oid">
            <column name="oid" sql-type="int(10) unsigned"/>
            <generator class="native"/>
        </id>
        <property name="type">
            <column name="type" sql-type="varchar(50)" not-null="true"/>
        </property>
        <property name="value">
            <column name="value" sql-type="varchar(100)" not-null="true"/>
        </property>

    </class>
</hibernate-mapping>