<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.bo.ewm.entity.Muban" table="e_muban" schema="ewm">
        <id name="oid">
            <column name="oid" sql-type="int(10) unsigned"/>
            <generator class="native"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(50)"/>
        </property>
        <property name="filedName">
            <column name="filedName" sql-type="varchar(50)"/>
        </property>
        <property name="filedType">
            <column name="filedType" sql-type="varchar(50)"/>
        </property>
        <property name="filedItem">
            <column name="filedItem" sql-type="varchar(50)"/>
        </property>

    </class>
</hibernate-mapping>